
max-items 25
browser "librewolf %u"

# Vim bindings
bind-key j down
bind-key k up
bind-key j next articlelist
bind-key k prev articlelist
bind-key J next-feed articlelist
bind-key K prev-feed articlelist
bind-key l open
bind-key h quit

# watch video
macro y set browser "mpv %u" ; open-in-browser ; set browser "librewolf %u"
